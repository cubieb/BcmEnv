    zxAnCmcNotifications     OBJECT IDENTIFIER ::= { zxAnCmcMib 10 }
 
    zxAnCmcExpandTrap     OBJECT IDENTIFIER ::= { zxAnCmcNotifications 1 }
    
    zxAnExtCmcUpstreamErrorRateAlm NOTIFICATION-TYPE
        OBJECTS { zxAnCmcUpstreamErrorRate, zxAnExtCmcUpstreamErrorRateThreshold}
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnCmcUpstreamErrorCodeRate object of the CMC has exceeded than the 
            configured threshhold value of zxAnExtCmcUpstreamErrorRateThreshold."
       ::= { zxAnCmcExpandTrap 1 }  

    zxAnExtCmcUpstreamErrorRateClr NOTIFICATION-TYPE
        OBJECTS { zxAnCmcUpstreamErrorRate, zxAnExtCmcUpstreamErrorRateThreshold }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnCmcUpstreamErrorCodeRate object of the CMC has less than the 
            configured threshhold value of zxAnExtCmcUpstreamErrorRateThreshold."
       ::= { zxAnCmcExpandTrap 2 }       
 
    zxAnExtCmcCmOnlineNotification NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCmMacAddress}
        STATUS  current
        DESCRIPTION
           "It indicates that the cm online."
       ::= { zxAnCmcExpandTrap 3 }  

    zxAnExtCmcCmOfflineNotification NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCmMacAddress}
        STATUS  current
        DESCRIPTION
           "It indicates that the cm offline."
       ::= { zxAnCmcExpandTrap 4 }
       
    zxAnExtCmcUpstreamLowSNRAlm NOTIFICATION-TYPE
        OBJECTS { zxAnCmcUpstreamSnr, zxAnExtCmcUpstreamSnrThreshold}
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnCmcUpstreamSnr object of the CMC has less than the 
            configured threshhold value of zxAnExtCmcUpstreamSnrThreshold."
       ::= { zxAnCmcExpandTrap 5 }  

    zxAnExtCmcUpstreamLowSNRClr NOTIFICATION-TYPE
        OBJECTS { zxAnCmcUpstreamSnr, zxAnExtCmcUpstreamSnrThreshold }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnCmcUpstreamSnr object of the CMC has exceeded than the 
            configured threshhold value of zxAnExtCmcUpstreamSnrThreshold."
       ::= { zxAnCmcExpandTrap 6 }           
   
    zxAnExtCmcCmExceedAlm NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCmNumber, zxAnExtCmcMaxCmThreshold}
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnExtCmcCmNumber object of the CMC has exceeded than the 
            configured threshhold value of zxAnExtCmcMaxCmThreshold."
       ::= { zxAnCmcExpandTrap 7 }  

    zxAnExtCmcCmExceedClr NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCmNumber, zxAnExtCmcMaxCmThreshold }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that the
            zxAnExtCmcCmNumber object of the CMC has less than the 
            configured threshhold value of zxAnExtCmcMaxCmThreshold."
       ::= { zxAnCmcExpandTrap 8 } 
   
    zxAnExtCmcDdosAttackAlm NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcDdosAttackType }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that 
            the CMC is under DDoS attack.
            The zxAnExtCmcDdosAttackType object of the CMC 
            has indicated the type of the attack."
       ::= { zxAnCmcExpandTrap 9 }  

    zxAnExtCmcDdosAttackClr NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcDdosAttackType }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that 
            the CMC is not under DDoS attack any more.
            The zxAnExtCmcDdosAttackType object of the CMC 
            has indicated the type of the attack."
       ::= { zxAnCmcExpandTrap 10 } 
                                     
    zxAnExtCmcUpstreamProfileChgAlm NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcUpChannelFormerProfile, zxAnExtCmcUpChannelCurrentProfile}
        STATUS  current
        DESCRIPTION
           "It indicates that the agent detected that the
            upstream profile CMC has been automatically changed."
       ::= { zxAnCmcExpandTrap 11 }  

    zxAnExtCmcUpstreamProfileChgClr NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcUpChannelFormerProfile, zxAnExtCmcUpChannelCurrentProfile }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent detected that the
            upstream profile CMC has been manually changed."
       ::= { zxAnCmcExpandTrap 12 } 
    zxAnExtCmcCardOfflineAlm NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCardOfflineType }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that 
            the DM card if offline."
       ::= { zxAnCmcExpandTrap 15 }  

    zxAnExtCmcCardOfflineClr NOTIFICATION-TYPE
        OBJECTS { zxAnExtCmcCardOfflineType }
        STATUS  current
        DESCRIPTION
           "It indicates that the agent has detected that 
            the DM card if online."
       ::= { zxAnCmcExpandTrap 16 } 
                                     
