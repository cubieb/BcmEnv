            FROM DOCS-IF-MIB
        enterprises
            FROM RFC1155-SMI;

    zxAnCmcMib MODULE-IDENTITY 
        LAST-UPDATED "2012101914Z"
        ORGANIZATION
            "ZTE Corporation"
        CONTACT-INFO
            "zhen weimin ZTE Corporation
             Mail: zhen.weimin@zte.com.cn
             Tel : +86-21-68897359"
        DESCRIPTION
            "This MIB defines DOCSIS CMC node managed objects."
        ::=  { zxAn 9026 }

    zte OBJECT IDENTIFIER   ::= { enterprises 3902 }

