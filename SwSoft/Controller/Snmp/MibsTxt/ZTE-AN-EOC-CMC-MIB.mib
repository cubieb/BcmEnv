    zxCableDocsLoadBalSystem       OBJECT IDENTIFIER ::= {zxCableDocsLoadBalObjects 1}
        
        zxCableDocsLoadBalMethod      OBJECT-TYPE 
        SYNTAX          Integer32 { Disabled ( 0 ) , Dynamic ( 1 ), Static ( 2 ) } 
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "Dynamic load balancing method.
                          0: Disable
                          1: Dynamic(run-time statistics)
                          2: Static(minimum reserved rate)"
        ::=  {  zxCableDocsLoadBalSystem  1  }
    
        zxCableDocsLoadBalPeriod   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "Time in seconds between load balancing checks. Minimum time is 30 seconds. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  2  }   

        zxCableDocsLoadBalNumPeriods   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "Number of periods for calculating the weighted average load. 
        Each period gets twice the weight of the previous period. 
        Valid values are 1, 2, or 4. Applies to Balancing Method 1 only. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  3  }   

        zxCableDocsLoadBalOverloadThreshold   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "A channel is considered overloaded when the load is at least this percent of capacity. 
        Below this, no attempt is made to balance its load. Applies to Balancing Method 1 only. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  4  }   

        zxCableDocsLoadBalDifferenceThreshold   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "For channels at or above the overload threshold, only consider less loaded alternate channels 
        where the load difference is at least this percent. Applies to Balancing Method 1 only. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  5  }   

        zxCableDocsLoadBalMaxMovesPerPeriod   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "Maximum number of modems allowed to be moved per load balancer period. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  6  }   
        
        zxCableDocsLoadBalMinHoldTimePerModem   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "Minimum number of seconds that must elapse before moving the same modem to a different channel. (0=no change)"
        ::=  {  zxCableDocsLoadBalSystem  7  }   
        
        zxCableDocsLoadBalRangingOverrideMode   OBJECT-TYPE 
        SYNTAX          Integer32 { Enabled ( 1 ) , Disabled ( 2 ) } 
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "When enabled, a modem can be directed to a different channel via RNG-RSP prior to registration (0=no change)"
        DEFVAL { enable } 
        ::=  {  zxCableDocsLoadBalSystem  8  }   
        
        zxCableDocsLoadBalDCCInitTechATDMA   OBJECT-TYPE 
