            zxAnCmcCmCpeIpAddressType     InetAddressType,
            zxAnCmcCmCpeIpAddress         InetAddress,
            zxAnCmcCmCpeType              Integer32
        }
        
    zxAnCmcCmIndex  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION     
            "The cm id."
        ::=  {  zxAnCmcCmCpeEntry  1  }
        
    zxAnCmcCmCpeIndex  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION     
            "The cpe id of the CM UNI port."
        ::=  {  zxAnCmcCmCpeEntry  2  }
        
    zxAnCmcCmMacAddress  OBJECT-TYPE 
        SYNTAX  MacAddress
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "The MAC address of the cm."
        ::=  {  zxAnCmcCmCpeEntry  3  }
        
    zxAnCmcCmCpeMacAddress  OBJECT-TYPE 
        SYNTAX  MacAddress
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "The MAC address of the cpe."
        ::=  {  zxAnCmcCmCpeEntry  4  }
        
