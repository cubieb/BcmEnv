    zxAnCmcCmUniTable  OBJECT-TYPE
        SYNTAX  SEQUENCE  OF  ZxAnCmcCmUniEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION 
            "This table show the user infomation of the CM UNI port."
        ::=  { zxAnCmcCmObjects  2 }

    zxAnCmcCmUniEntry  OBJECT-TYPE
        SYNTAX  ZxAnCmcCmUniEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION 
            "An entry in zxAnCmcCmUniTable."
        INDEX  {  docsIfCmtsCmStatusIndex, zxAnCmcCmUserMacAddress  }
        ::=  { zxAnCmcCmUniTable  1 }

    ZxAnCmcCmUniEntry  ::=  SEQUENCE 
        {
            zxAnCmcCmUserMacAddress     MacAddress,
            zxAnCmcCmUserIpAddressType  InetAddressType,
            zxAnCmcCmUserIpAddress        InetAddress,
            zxAnCmcCmConnectState         Integer32,
            zxAnCmcCmOnlineTime           TimeTicks,
            zxAnCmcCmDocsisVersion        Integer32,
            zxAnCmcCmTimeOffSet           Integer32,
            zxAnCmcCmMicroReflect         Integer32,
            zxAnCmcCmPrimaryUsChannel     Integer32,
            zxAnCmcCmPrimaryDsChannel     Integer32,
            zxAnCmcCmUpstreamSNR          Integer32,
            zxAnCmcCmUpstreamPower        Integer32,            
            zxAnCmcCmDownstreamSNR        Integer32,
            zxAnCmcCmDownstreamPower      Integer32,
            zxAnCmcCmNumberOfCPEs         Integer32,
            zxAnCmcCmConfigFile           OCTET STRING,
            zxAnCmcCmResetNow             TruthValue,
            zxAnCmcCmDeleteNow            TruthValue,
            zxAnCmcCmUpstreamBandWidth    Integer32,
            zxAnCmcCmDownstreamBandWidth  Integer32
        }

    zxAnCmcCmUserMacAddress  OBJECT-TYPE 
        SYNTAX  MacAddress
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION     
            "The MAC address of the user device."
        ::=  {  zxAnCmcCmUniEntry  1  }

    zxAnCmcCmUserIpAddressType  OBJECT-TYPE 
        SYNTAX  InetAddressType
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "The type of IP address used by this entry."
        DEFVAL { ipv4 }
        ::=  {  zxAnCmcCmUniEntry  2  }
        
    zxAnCmcCmUserIpAddress  OBJECT-TYPE 
        SYNTAX  InetAddress
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "The IP address of the user device."
        ::=  {  zxAnCmcCmUniEntry  3  }

    zxAnCmcCmConnectState  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
           "The CM connectivity state: 
            Other =1
            InitialRanging =2
            RangingAutoAdjComplete = 4
            startEae =10
            startDhcpv4 = 11
            startDhcpv6 =12
            Dhcpv4Complete = 5
            Dhcpv6Complete = 13
            StartConfigFileDownload = 14
            configFileDownloadComplete = 15
            startRegistration = 16
            registrationComplete =6
            operational =8
            BpiInit =9
            forwardingDisabled = 17
            rfMuteAll = 18"
        ::=  {  zxAnCmcCmUniEntry  4  }

    zxAnCmcCmOnlineTime  OBJECT-TYPE 
        SYNTAX  TimeTicks
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
           "the cable modem online time"
        ::=  {  zxAnCmcCmUniEntry  5  }

    zxAnCmcCmDocsisVersion  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
           "the cable modem DOCSIS version:
            1  Docsis10
            2  Docsis11
            3  Docsis20
            4  Docsis30"
        ::=  {  zxAnCmcCmUniEntry  6  }

    zxAnCmcCmTimeOffSet  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
           "the cable modem timing offset. units : millisecond."
        ::=  {  zxAnCmcCmUniEntry  7  }

    zxAnCmcCmMicroReflect  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
           "Total Micro-reflections measured in dBc"
        ::=  {  zxAnCmcCmUniEntry  8  }
                        
    zxAnCmcCmPrimaryUsChannel   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "This attribute contains the primary upstream channel,which the cable modem belong to."
        ::=  {  zxAnCmcCmUniEntry  9  }       

    zxAnCmcCmPrimaryDsChannel   OBJECT-TYPE 
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "This attribute contains the primary downstream channel,which the cable modem belong to."
        ::=  {  zxAnCmcCmUniEntry  10  }   

    zxAnCmcCmUpstreamSNR  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the cable modem upstream SNR.unit:tenthdB"
        ::=  {  zxAnCmcCmUniEntry  11  }
        
    zxAnCmcCmUpstreamPower  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the cable modem upstream send power.unit:tenthdBmV"
        ::=  {  zxAnCmcCmUniEntry  12  }
        
    zxAnCmcCmDownstreamSNR  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the cable modem downstream SNR.unit:tenthdB"
        ::=  {  zxAnCmcCmUniEntry  13  }
        
    zxAnCmcCmDownstreamPower  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the cable modem downstream receive power.unit:tenthdBmV"
        ::=  {  zxAnCmcCmUniEntry  14  }
                
    zxAnCmcCmNumberOfCPEs  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the number of CPEs in the cable modem."
        ::=  {  zxAnCmcCmUniEntry  15  }
        
      zxAnCmcCmConfigFile  OBJECT-TYPE 
        SYNTAX  OCTET STRING (SIZE (128))
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "The config file cable modem config file."
        ::=  {  zxAnCmcCmUniEntry  16  }       
                 
      zxAnCmcCmResetNow OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Setting this object to true(1) causes the device to
             reset.  Reading this object always returns false(2)."
        ::=  {  zxAnCmcCmUniEntry  17  }
        
      zxAnCmcCmDeleteNow OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Setting this object to true(1) causes the device to
             re-register.  Reading this object always returns false(2)."
        ::=  {  zxAnCmcCmUniEntry  18  }
        
      zxAnCmcCmUpstreamBandWidth  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the upstream bandwidth of one cm.unit:bps(bit per second)"
        ::=  {  zxAnCmcCmUniEntry  19  }
        
      zxAnCmcCmDownstreamBandWidth  OBJECT-TYPE 
        SYNTAX  Integer32
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION     
            "the downstream bandwidth of one cm.unit:bps(bit per second)"
        ::=  {  zxAnCmcCmUniEntry  20  }
